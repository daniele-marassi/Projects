{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "AppSettings": {
    //system configs - START
    //"BaseUrl": "http://IP:82",
    //"BaseUrl": "http://localhost:52110",
    "BaseUrl": "http://192.168.1.105:82",
    "LimitLogFileInMB": 50,
    //system configs - END
    //GeneralSettings - START
    "PageSize": "3",
    "Culture": "it-IT",
    //GeneralSettings - END
    //Speech - START
    "HostsArray": "[\"EV-PC\",\"EV-TB\"]",
    "HostDefault": "EV-PC",
    "ListeningWord1": "ehi",
    "ListeningWord2": "box",
    "ListeningAnswer": "si dimmi",
