    //Salutation - if it contains the key 'SURNAME' it will be replaced with your profile surname
    "Salutation": "Ehi NAME",
    "MinSpeechWordsCoefficient": "0,05",
    "MaxSpeechWordsCoefficient": "0,05",
    //MeteoParameterToTheSalutation - empty to disable it
    "MeteoParameterToTheSalutation": "trieste-oggi-32006",
    "DescriptionMeteoToTheSalutationActive": "True",
    "TimesToReset": 600
    //Speech - END
  },
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://localhost:5200"
      }
    }
